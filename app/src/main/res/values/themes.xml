    <style name="Theme.Stardeos.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Stardeos.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>